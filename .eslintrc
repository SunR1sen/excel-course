{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"linebreak-style": 0,
		"global-require": 1,
		"eslint linebreak-style": [0, "error", "windows"],
		"arrow-parens":"off",
		"require-jsdoc":"off",
		"indent":"off",
		"no-tabs":"off",
		"operator-linebreak":"off"
	},
	"extends": ["eslint:recommended", "google"]
}